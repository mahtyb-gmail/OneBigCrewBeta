<h3>Candidates</h3>
<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>email</th>
			<th>Title</th>
			<th>Location</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	
	<tbody>
		<% @users.each do |user| %>
		<tr>
		  <td><%= user.name %></td>
		  <td><%= user.email %></td>
		  <td><%= user.title %></td>
		  <td><%= user.location %></td>
		  <td><%= link_to 'Show', controller: "users", action: "show", id: user.id %></td>
		</tr>
		<% end %>
	</tbody>
</table>
	
